{
  "env": {
    "browser": false,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions" :"off",
    "no-shadow": "off",
    "no-console": "off",
    "no-param-reassign": "off"
  }
}
